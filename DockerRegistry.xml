<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Date>2018-06-01</Date>
  <Beta>False</Beta>
  <Category>Docker</Category>
  <Name>DockerRegistry</Name>
  <Project>https://hub.docker.com/_/registry/</Project>
  <Overview>
    Docker Registry stores Docker images
  </Overview>
  <Description>
    The Docker Registry allows you to run a private Docker Registry instance that you can push your images into.[br]
    [b][span style='color: #E80000;']Required:[/span][/b][br]
    [b]/var/lib/registry[/b] : this is where Docker Registry will store it's databases and configuration.[br][br]
  </Description>
  <Registry>https://hub.docker.com/_/registry/</Registry>
  <GitHub>https://hub.docker.com/_/registry/</GitHub>
  <Repository>registry</Repository>
  <Support>https://github.com/pbinks/unraid-docker-registry/issues</Support>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>host</Mode>
    <Publish/>
  </Networking>
  <Environment/>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/DockerRegistry</HostDir>
      <ContainerDir>/var/lib/registry</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI/>
  <Banner/>
  <Icon/>
</Containers>
